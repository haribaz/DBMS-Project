<div class="grid-container">
	<% for(let i=0; i<10; i++) { %>
	<div class="movie-card card">
		<img
			src="https://i.pinimg.com/originals/d8/b6/f4/d8b6f4918c3d0e6ebc8440f8ab6c58a2.jpg"
			class="card-img-top"
			alt="..."
		/>
		<div class="text-center card-body">
			<h5 class="card-title movie-title">Card title</h5>
			<div class="movie-btn-grid">
				<button
					class="btn btn-primary"
					data-bs-toggle="collapse"
					data-bs-target="#collapse<%= i %>"
				>
					See More
				</button>
				<a href="/user/showMovie" class="movie-see-more btn btn-primary">Movie Page</a>
			</div>
			<div class="collapse text-primary mt-3" id="collapse<%= i %>">
				<div class="card-body">
					Actor: Vijay
					<br />
					Director: Atle
				</div>
			</div>
		</div>
	</div>
	<% } %>
</div>
